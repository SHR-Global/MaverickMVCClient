<configuration>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
      
    <!-- 1. Replace these values with your Maverick configuration -->
    <add key="maverick:ClientId" value="" /> <!-- Request this from Maverick Team -->
    <add key="maverick:ClientSecret" value="" />  <!-- Request this from Maverick Team -->
    <add key="maverick:AuthUri" value="https://idserveruat.maverickcrm.com" />
    <add key="maverick:APIUri" value="https://crmuat.maverickcrm.com/rest2" />
    <add key="maverick:pointsCurrencyCode" value="USD"/>
      
    <!-- 2. Update the Maverick Client application setup with these values -->
    <add key="maverick:RedirectUri" value="https://localhost:44326/signin-oidc?hotelGroupCode={0}" />
    <add key="maverick:PostLogoutRedirectUri" value="http://localhost:44326/Account/PostLogout" />

    <!-- 3. Update the Windsurfer Booking Engine values -->
    <add key="windsurfer:IBEUri" value="https://uat.windsurfercrs.com/ibe" />
    <add key="windsurfer:hotelGroupCode" value="MAVDEMOPTS" />  <!-- Windsufer Chain Code -->
    <add key="windsurfer:hotelGroupID" value="1040" /> <!-- Windsufer Chain ID -->
    <add key="windsurfer:hotelCode" value="MAVCRM4" /> <!-- Windsufer Hotel Code -->

  </appSettings>
  <system.web>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.8" />
    <httpRuntime targetFramework="4.8" />
  </system.web>
  <system.webServer>
